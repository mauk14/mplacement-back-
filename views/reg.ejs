<!DOCTYPE html>
<html lang="en">
<head>
<title>Sweet Street</title>
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
  <link rel="stylesheet" href="public/styles/styles.css" />
  <script src="public/scripts/store.js" async></script>
  <link rel="icon" type="image/x-icon" href="public/img/favicon.ico">

  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
</head>
<body>
    <%- include ('nav.ejs') %>


    <section class="container content-section">
            <br> 
            <h2 class="section-header">Registration</h2>
            <br>
            <div class="card">

                <form method="post" action="/">
                    <div class="form-group">
                      <label for="login">Username</label>
                      <input type="text" class="form-control" id="login" placeholder="Enter username" name="login">
                      <label for="email">Email address</label>
                      <input type="email" class="form-control" id="email" placeholder="Enter email" name="email">
                      <label for="password">Password</label>
                      <input type="password" class="form-control" id="password" placeholder="Password" name="pass">
                      <label for="repitPassword">Repit Password</label>
                      <input type="password" class="form-control" id="repitPassword" placeholder="Repit Password" name="pass2">
                    </div>
                    <div class="form-check">
                      <input type="checkbox" class="form-check-input" id="rules" name="rule">
                      <label class="form-check-label" for="rules">You agree to our rules</label>
                    </div>
                    <button type="submit" class="btn btn-primary">Submit</button>
                  </form>
            
            </div>

        </section>
</body>
</html>